<?xml version='1.0' encoding='utf-8' ?>
<odoo>
    <record id="sale_order_form_quote" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.sale_management</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//field[@name='order_line']//tree/field[@name='product_template_id']"
                position="before"
            >
                <field name="product_template_id_cpq_ok" invisible="1" />
            </xpath>
        </field>
    </record>
</odoo>
