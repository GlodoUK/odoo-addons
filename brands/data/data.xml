<odoo noupdate="1">
    <record id="main_brand" model="glo.brand">
        <field name="name">Your Company Brand</field>
        <field name="partner_id" ref="base.main_partner" />
        <field name="is_default">True</field>
    </record>

    <record id="other_brand_partner" model="res.partner">
        <field name="name">Other Partner Brand</field>
        <field name="street">Address 1</field>
        <field name="street2">Address 2</field>
        <field name="city">City</field>
        <field name="zip">AA1 1AA</field>
        <field name="country_id" ref="base.uk" />
        <field name="phone">01234 567890</field>
        <field name="website">https://www.somebranddomain.com/</field>
        <field name="email">info@somebranddomain.com</field>
    </record>

    <record id="other_brand" model="glo.brand">
        <field name="name">Other Partner Brand</field>
        <field name="partner_id" ref="other_brand_partner" />
    </record>
</odoo>
